class Solution:
    def twoSum(self, nums: List[int], target: int) -> List[int]:
        for i in range(len(nums)):
            if nums[i] == 0:
                i+=1
            else:
                a = nums[i]
                b = nums[i+1]
                if a+b==target:
                    return [i,i+1]
                else:
                    i+=1
            

            
    nums = [2,7,11,15]
    target = 9
